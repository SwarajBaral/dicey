{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "strict": true,
    "outDir": "dist",
    "skipLibCheck": true,
    "lib": [
      "dom",
      "es2015.promise",
      "es5"
    ]
  },
  "include": [
    "./src/**/*",
    "./src/*"
  ]
}